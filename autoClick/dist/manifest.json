{
  "manifest_version": 2,
  "name": "AutoClick",
  "version": "1.0.0",
  "description": "自动点击刷网课",
  "browser_action": {
    "default_icon": "./icons/autoClick-64.png",
    "default_title": "AutoClick",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./icons/autoClick-16.png",
    "64": "./icons/autoClick-64.png",
    "128": "./icons/autoClick-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.min.js",
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://www.huanggaofang.com",
  "omnibox": {
    "keyword": "autoClick"
  },
  "default_locale": "zh_CN"
}